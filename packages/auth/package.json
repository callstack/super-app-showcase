{
  "name": "auth",
  "version": "0.0.1",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "start": "react-native webpack-start --port 9003",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
